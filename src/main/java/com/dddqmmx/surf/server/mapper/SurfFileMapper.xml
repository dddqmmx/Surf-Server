<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dddqmmx.surf.server.mapper.SurfFileMapper">

    <select id="getFileByMD5" resultType="com.dddqmmx.surf.server.pojo.SurfFile">
        select * from surf_file where md5 = #{md5}
    </select>

    <select id="getFileById" resultType="com.dddqmmx.surf.server.pojo.SurfFile">
        select * from surf_file where id = #{id}
    </select>
</mapper>