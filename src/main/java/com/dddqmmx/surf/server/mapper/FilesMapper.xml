<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dddqmmx.surf.server.mapper.FilesMapper">

    <select id="getFileByMD5" resultType="com.dddqmmx.surf.server.pojo.Files">
        select * from files where md5 = #{md5}
    </select>
</mapper>